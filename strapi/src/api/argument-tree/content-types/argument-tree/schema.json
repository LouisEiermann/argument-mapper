{
  "kind": "collectionType",
  "collectionName": "argument_trees",
  "info": {
    "singularName": "argument-tree",
    "pluralName": "argument-trees",
    "displayName": "Argument Tree",
    "description": ""
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string"
    },
    "mood": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "nodes": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::node.node"
    },
    "creator": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.user",
      "inversedBy": "created"
    },
    "opponent": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.user",
      "inversedBy": "isOpponent"
    },
    "opponentAccepted": {
      "type": "boolean",
      "default": false
    },
    "tags": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::tag.tag"
    },
    "description": {
      "type": "text"
    },
    "votes": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::vote.vote",
      "mappedBy": "argumentTree"
    }
  }
}
